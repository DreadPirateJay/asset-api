	},
	"require": {
		"phpunit/phpunit": "3.7.x",
		"mockery/mockery": "dev-master",
		"zizaco/factory-muff": "dev-master",
		"way/generators": "dev-master",
		"way/laravel-test-helpers": "dev-master"
